{
  "name": "parcel-react-starter",
  "version": "1.0.0",
  "description": "React + Parcel + ESLint + VSCode + SCSS + JEST + React Testing Library",
  "source": "index.html",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "parcel build index.html",
    "serve": "parcel serve --open",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix && yarn format",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress": "cypress open",
    "format": "prettier --write src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanishraj/Interview-Exercise.git"
  },
  "author": "Tanishraj",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tanishraj/Interview-Exercise/issues"
  },
  "homepage": "https://github.com/tanishraj/Interview-Exercise#readme",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@parcel/transformer-sass": "2.12.0",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-hooks": "^8.0.1",
    "babel-jest": "^29.7.0",
    "cypress": "^13.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "parcel": "^2.12.0",
    "prettier": "^3.2.5",
    "process": "^0.11.10"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  }
}
